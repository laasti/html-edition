{
    "name": "laasti/html-edition",
    "description": "",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sonia Marquette",
            "email": "contact@nebulousweb.com",
            "homepage": "http://nebulousweb.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/laasti/html-edition/issues",
        "wiki": "https://github.com/laasti/html-edition/wiki"
    },
    "require": {
        "php" : "~5.6|~7.0",
        "laasti/symfony-translation-provider": "^2.0.1",
        "laasti/views": "^0.2.2",
        "laasti/application": "^0.2.1",
        "laasti/peels": "^0.2.1",
        "laasti/directions": "^0.4.1",
        "zendframework/zend-diactoros": "^1.3.3",
        "league/container": "^2.2",
        "monolog/monolog": "^1.17.2",
        "league/booboo": "^1.0.0",
        "laasti/lazydata": "^0.6.1",
        "mustache/mustache": "^2.11.0",
        "hassankhan/config": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Laasti\\HtmlEdition\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
